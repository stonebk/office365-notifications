{
  "name": "Outlook Notifier",
  "description": "Outlook 365 Notification",
  "version": "2.0",
  "permissions": [
    "*://*/*",
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Outlook Notifier",
    "default_icon": "outlook_32.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "outlook_256.png"
  ]
}
